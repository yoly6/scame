@model SCAME.Models.Medico

@{
    ViewData["Title"] = "Details";
}
<br/>
<br/>
<br/>
<section id="hero" class="d-flex align-items-center">
    <div class="container">
      <h1>Detalle del Medico</h1>
      <h2>
      <br/>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cedula)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cedula)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nombre) @Html.DisplayFor(model => model.Apellido)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Telefono)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Telefono)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodigoSenecyt)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodigoSenecyt)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TituloEgresado)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TituloEgresado)
        </dd>
    </dl>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn-get-started scrollto">Editar</a>
    <a asp-action="Index" class="btn-get-started scrollto">Regresar</a>
</div>
</h2>
 </div>
  </section>
