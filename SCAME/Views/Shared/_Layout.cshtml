<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SCAME</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/components.css">
    <link rel="stylesheet" href="~/css/icons.css">
    <link rel="stylesheet" href="~/css/responsee.css">
    <link rel="stylesheet" href="~/owl-carousel/owl.carousel.css">
    <link rel="stylesheet" href="~/owl-carousel/owl.theme.css">
    <link rel="stylesheet" href="~/css/template-style.css">

    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,700,900&amp;subset=latin-ext" rel="stylesheet">
    <script type="text/javascript" src="~/js/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/js/jquery-ui.min.js"></script>

    <link rel="stylesheet" href="~/homeAdmin/css/style.css">
    <link rel="stylesheet" href="~/homeAdmin/css/material-design-iconic-font.min.css">


</head>
<body class="size-1280">
    <header role="banner" class="position-relative">
        <nav class="background-primary-dott full-width section-top-small-padding text-padding-small sticky ">
            <div class="top-nav">
                <!-- mobile version logo -->
                <div class="logo hide-l hide-xl hide-xxl">
                    <a asp-area="" asp-controller="Home" asp-action="Index" class="logo">
                        <!-- Logo White Version -->
                        <img class="logo-white" src="img/logo.svg" alt="">
                        <!-- Logo Dark Version -->
                        <img class="logo-dark" src="img/logo-dark.svg" alt="">
                    </a>
                </div>
                <p class="nav-text"></p>

                <div class="top-nav left-menu">

                    <ul class="right top-ul chevron">
                        <li><a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a></li>
                        <li><a asp-area="" asp-controller="Home" asp-action="Privacy">Privacidad</a></li>
                        <li><a>Otros Servicios</a></li>
                    </ul>
                </div>

                <!-- logo -->
                <div class="logo-menu">
                    <a asp-area="" asp-controller="Home" asp-action="Index" class="logo">
                        <!-- Logo White Version -->
                        <img class="logo-white" src="img/logo.svg" alt="">
                        <!-- Logo Dark Version -->
                        <img class="logo-dark" src="img/logo-dark.svg" alt="">
                    </a>
                </div>

                <!-- right menu items -->
                <div class="top-nav right-menu">

                    <ul class="top-ul chevron">
                        <partial name="_LoginPartial" />
                    </ul>

                </div>
            </div>
        </nav>
    </header>
    <main role="main">
        @RenderBody()
    </main>

    <footer class="border-top footer text-muted background-dark">
        <div class="container">
            &copy; 2020 - SCAME 
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script type="text/javascript" src="~/js/responsee.js"></script>
    <script type="text/javascript" src="~/owl-carousel/owl.carousel.js"></script>
    <script type="text/javascript" src="~/js/template-scripts.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
